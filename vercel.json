{
  "crons": [
    {
      "path": "/api/poll",
      "schedule": "*/5 * * * * *"
    }
  ],
  "env": {
    "YT_API_KEY": "@yt_api_key",
    "GOOGLE_SERVICE_ACCOUNT": "@google_service_account",
    "SHEET_ID": "@sheet_id",
    "TWJ_SHEET_ID": "@twj_sheet_id",
    "IS_SHEET_ID": "@is_sheet_id",
    "MONGODB_URI": "@mongodb_uri",
    "CRON_SECRET": "@cron_secret"
  }
}
